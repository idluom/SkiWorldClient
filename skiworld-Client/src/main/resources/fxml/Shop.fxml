                        <TableColumn fx:id="price1" prefWidth="73.0" text="Price" />
                        <TableColumn fx:id="invq1" prefWidth="115.0" text="Inventory Quantity" />
                    </columns>
                  </TableView>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <TextField fx:id="text" layoutX="100.0" layoutY="71.0" promptText="Search..." AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="70.0" />
      <Button fx:id="del" layoutX="149.0" layoutY="384.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="30.0" styleClass="btn" text="Delete Equipement" AnchorPane.bottomAnchor="40.0" AnchorPane.rightAnchor="200.0" />
