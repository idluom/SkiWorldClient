      <Button fx:id="act" layoutX="736.0" layoutY="46.0" mnemonicParsing="false" onAction="#refresh" prefHeight="30.0" styleClass="btn" text="Refresh" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="70.0" />
      <Button fx:id="move" layoutX="717.0" layoutY="137.0" mnemonicParsing="false" onAction="#moveToShop" styleClass="btn" text="Move To Shop" AnchorPane.leftAnchor="720.0" AnchorPane.rightAnchor="50.0" />
      <TextField fx:id="Tq" layoutX="717.0" layoutY="183.0" prefHeight="30.0" prefWidth="93.0" promptText="Quantity" AnchorPane.leftAnchor="720.0" AnchorPane.rightAnchor="50.0" />
      <Text layoutX="436.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Shop" AnchorPane.topAnchor="20.0" />
